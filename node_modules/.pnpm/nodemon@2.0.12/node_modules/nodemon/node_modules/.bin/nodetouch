#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/touch@3.1.0/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules:/mnt/c/Users/hunter/IdeaProjects/node_modules:/mnt/c/Users/hunter/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/nodemon@2.0.12/node_modules/nodemon/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/nodemon@2.0.12/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/bin/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/touch@3.1.0/node_modules/touch/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/touch@3.1.0/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules:/mnt/c/Users/hunter/IdeaProjects/node_modules:/mnt/c/Users/hunter/node_modules:/mnt/c/Users/node_modules:/mnt/c/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/nodemon@2.0.12/node_modules/nodemon/node_modules:/mnt/c/Users/hunter/IdeaProjects/bel_node_server/node_modules/.pnpm/nodemon@2.0.12/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../touch@3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
else
  exec node  "$basedir/../../../../../touch@3.1.0/node_modules/touch/bin/nodetouch.js" "$@"
fi
